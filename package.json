{
  "name": "character-count",
  "description": "Count characters, words, and paragraphs from a text layer easily inside Sketch!",
  "version": "0.0.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "Character Count",
    "manifest": "src/manifest.json",
    "main": "Character Count.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afnizarnur/character-count.git"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "author": "Afnizar Nur Ghifari <afnizarhilmi@gmail.com>",
  "bugs": {
    "url": "https://github.com/afnizarnur/character-count/issues"
  },
  "homepage": "https://github.com/afnizarnur/character-count#readme",
  "main": "index.js",
  "dependencies": {},
  "license": "MIT",
  "keywords": [
    "sketch-plugin",
    "sketch",
    "collaboration",
    "copywriting"
  ]
}
